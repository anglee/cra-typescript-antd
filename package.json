{
  "name": "cra-typescript-antd",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "^3.2.0",
    "@types/jest": "23.3.9",
    "@types/node": "10.12.10",
    "@types/react": "16.7.7",
    "@types/react-dom": "16.0.11",
    "antd": "^3.11.2",
    "craco-antd": "^1.9.0",
    "randomcolor": "^0.5.3",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-scripts": "2.1.1",
    "tslint": "^5.11.0",
    "typescript": "3.1.6"
  },
  "devDependencies": {
    "@types/randomcolor": "^0.5.0",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3",
    "tslint-config-prettier": "^1.17.0"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test:watch": "craco test",
    "test": "CI=true craco test",
    "prettier": "prettier --write './**/*.{ts,tsx,js,css,scss,json}'",
    "prepush": "run-s lint test",
    "tsc-lint": "tsc --noEmit -p tsconfig.prod.json",
    "tslint": "tslint -c tslint.prepush.json \"src/**/*.{ts,tsx}\"",
    "tslint:fix": "tslint -c tslint.prepush.json --fix \"src/**/*.{ts,tsx}\"",
    "lint": "run-s tslint tsc-lint",
    "fix": "run-s tslint:fix prettier",
    "eject": "react-scripts-ts eject"
  },
  "lint-staged": {
    "*.{ts,tsx,js,json,css,less,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "run-s prepush"
    }
  },
  "prettier": {
    "printWidth": 100,
    "trailingComma": "all"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
