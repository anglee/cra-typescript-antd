{
  "name": "cra-typescript-antd",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/jest": "24.0.15",
    "@types/node": "12.6.8",
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.5",
    "antd": "^3.23.3",
    "lodash": "^4.17.15",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "typescript": "3.5.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "eslint": "^5.0.1",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.2",
    "husky": "^3.0.1",
    "lint-staged": "^9.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "CI=true react-scripts test",
    "test:interactive": "react-scripts test",
    "prettier": "prettier --write './**/*.{ts,tsx,js,css,less,scss,json}'",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx src --ignore-pattern src/serviceWorker.ts",
    "eslint:fix": "eslint --ext .js,.jsx,.ts,.tsx src --ignore-pattern src/serviceWorker.ts --fix",
    "lint": "run-s eslint",
    "fix": "run-s eslint:fix prettier",
    "prepush": "run-s lint test",
    "eject": "react-scripts eject"
  },
  "prettier": {
    "printWidth": 100,
    "trailingComma": "all"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "run-s prepush"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "pretty-quick --staged",
      "git add"
    ],
    "*.{json,css,less,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
